<div id="cal-heatmap"></div>
	<script type="text/javascript">
		var cal = new CalHeatMap();
		cal.init({
			domain: "month",
			subDomain: "x_day",
			data: {"1456963200":1,"1456863200":1},
			cellSize: 20,
			cellPadding: 5,
			domainGutter: 20,
			range: 4,
			start: (new Date()).setMonth(new Date().getMonth() - 3),
			domainDynamicDimension: false,
			subDomainTextFormat: "%d",
			displayLegend: false,
			legend: [1, 2, 3, 4],
			highlight: "now"

		});
	</script>